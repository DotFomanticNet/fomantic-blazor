@namespace Fomantic.Blazor.UI
@inherits FomanticComponentBase

@code {

    protected RenderFragment RenderContent()
    {
        return __builder =>
        {
        @RenderBeforeContentFragments()
        <CascadingValue Name="Parent" Value="this">
            @ChildContent
        </CascadingValue>
        @RenderAfterContentFragments()
        };
    }
}
